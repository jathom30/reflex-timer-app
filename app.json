{
  "expo": {
    "name": "reflexing",
    "description": "A simple reflex app to test your reflexes.",
    "slug": "reflex-app",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": "ios",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#6C5CE7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeffthomas.reflexing"
    }
  }
}
